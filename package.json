{
  "name": "bookmarker",
  "version": "0.2.1",
  "description": "chrome extension for shared bookmarks with other users",
  "scripts": {
    "test": "jest",
    "start": "webpack serve --mode development",
    "build:dev": "rm -rf ./dist && webpack --mode development",
    "build": "rm -rf ./dist && webpack --mode production",
    "zip": "rm -f bookmarker.zip && cp -r dist bookmarker && zip -r bookmarker.zip bookmarker/* && rm -rf bookmarker",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "author": "areisle@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/jest": "^29.4.0",
    "@types/lodash.uniq": "^4.5.6",
    "@types/webextension-polyfill": "^0.8.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "dotenv-webpack": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^29.4.3",
    "msw": "^0.49.3",
    "msw-storybook-addon": "^1.7.0",
    "plop": "^2.7.4",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "browserslist": [
    "chrome >= 90"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@mui/system": "^5.11.9",
    "@tanstack/react-query": "^4.24.9",
    "@types/chrome": "^0.0.217",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "lodash.startcase": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}