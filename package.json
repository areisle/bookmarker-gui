{
    "name": "bookmarker",
    "version": "0.2.0",
    "description": "chrome extension for shared bookmarks with other users",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack serve --mode development",
        "build:dev": "rm -rf ./dist && webpack --mode development",
        "build": "rm -rf ./dist && webpack --mode production",
        "zip": "rm -f bookmarker.zip && cd dist && zip -r ../bookmarker.zip *",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "generate-types": "graphql-codegen --config codegen.yml"
    },
    "author": "areisle@gmail.com",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@graphql-codegen/cli": "2.2.0",
        "@graphql-codegen/typescript": "2.2.2",
        "@graphql-codegen/typescript-operations": "2.1.6",
        "@graphql-codegen/typescript-react-query": "^3.0.1",
        "@storybook/addon-actions": "^6.4.0-beta.3",
        "@storybook/addon-essentials": "^6.4.0-beta.3",
        "@storybook/addon-links": "^6.4.0-beta.3",
        "@storybook/react": "^6.4.0-beta.3",
        "@types/lodash.uniq": "^4.5.6",
        "@types/webextension-polyfill": "^0.8.0",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "dotenv-webpack": "^7.0.3",
        "html-webpack-plugin": "^5.3.2",
        "plop": "^2.7.4",
        "webextension-polyfill": "^0.8.0",
        "webpack": "^5.56.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.3.0"
    },
    "browserslist": [
        "chrome >= 90"
    ],
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5.0.3",
        "@mui/material": "^5.0.2",
        "@mui/system": "^5.0.2",
        "@types/react": "^17.0.27",
        "@types/react-dom": "^17.0.9",
        "date-fns": "^2.25.0",
        "firebase": "^9.1.3",
        "graphql": "^15.6.1",
        "lodash.uniq": "^4.5.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-query": "^3.25.1",
        "typescript": "4.4.3"
    }
}
